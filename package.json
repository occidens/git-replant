{
  "name": "git-replant",
  "version": "0.2.0",
  "description": "Rebase an entire git subtree to a new root",
  "bin": "src/cli.js",
  "main": "src/cli.js",
  "scripts": {
    "build" : "pkg --target=node10 --output=bin/git-replant.exe ./package.json"
  },
  "keywords": [
